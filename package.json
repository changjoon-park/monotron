{
  "name": "electron-python-monorepo",
  "version": "1.1.0",
  "description": "",
  "scripts": {
    "venv": "pnpm --filter='python' create:venv",
    "build": "pnpm --filter='python' install:python && pnpm --filter='python' bundle:python",
    "start:server": "pnpm --filter='python' start:dev",
    "start:electron": "pnpm --filter='my-electron*' start:dev",
    "test": "concurrently -k \"npm run start:server\" \"npm run start:electron\"",
    "package": "pnpm --filter='my-electron*' package"
  },
  "keywords": [
    "electron",
    "python",
    "monorepo",
    "boilerplate"
  ],
  "author": {
    "name": "Changjoon",
    "email": "sisyphus9402@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/changjoon-park/electron-python.git"
  },
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  }
}
